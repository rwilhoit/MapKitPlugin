<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    id="com.amazon.mapkit"
    version="0.1.0">

    <name>MapKit</name>
    <description>Cordova Plugin Sekelton</description>
    <license>Apache 2.0</license>
    <keywords>cordova,plugin,skeleton</keywords>

    <!-- ios -->
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="MapKit">
                <param name="ios-package" value="MapKit"/>
                <param name="onload" value="true" />
            </feature>
        </config-file>

        <js-module src="www/MapKit.js" name="MapKit">
            <clobbers target="mapkit" />
        </js-module>

        <header-file src="src/ios/MapKit.h" />
        <source-file src="src/ios/MapKit.m" />
        
    </platform>


</plugin>
